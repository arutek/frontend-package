{
  "name": "@arutek/package-styles",
  "private": true,
  "version": "0.0.1",
  "description": "All styling for Arutek application went here",
  "main": "./index.scss",
  "type": "module",
  "scripts": {
    "dev": "pnpx tailwindcss -i ./index.scss -o ./dist/tailwind.scss && pnpm compile",
    "dev:partner-sip-aja": "cp .env.sip-aja .env && pnpx tailwindcss -i ./index.scss -o ./dist/tailwind.scss && pnpm compile && rm .env",
    "build": "pnpx tailwindcss -i ./index.scss -o ./dist/tailwind.scss && pnpm compile",
    "build:partner-sip-aja": "cp .env.sip-aja .env && pnpx tailwindcss -i ./index.scss -o ./dist/tailwind.scss && pnpm compile && rm .env",
    "compile": "sass ./dist/tailwind.scss ./dist/tailwind.css --style compressed",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "contributors": [
    {
      "name": "Rizky Novando Priyadi",
      "email": "rizkynovando@gmail.com",
      "url": "https://gitlab.com/novando"
    }
  ],
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.23",
    "postcss-import": "^15.1.0",
    "sass": "^1.62.1",
    "tailwindcss": "^3.3.2"
  },
  "dependencies": {
    "dotenv": "^16.0.3"
  }
}
